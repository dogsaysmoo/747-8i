				<z-offset-m archive="y">-28.01</z-offset-m>
				<pitch-offset-deg archive="y">-90.0</pitch-offset-deg>
			</config>
		</view>
		
		<view n="1503">
			<name>Copilot</name>
			<type>lookfrom</type>
