<!--
Syd Adams
777-200 PFD 
-->

<PropertyList>

 <path>pfd.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>10</max-m>
 </animation>

<animation>
 <type>translate</type>
 <object-name>FDbars</object-name>
 <property>/instrumentation/flightdirector/bar-pitch</property>
 <factor>0.0016</factor>
 <axis>
  <x>0</x>
  <y>0</y>
  <z>1</z>
 </axis>
</animation>

<animation>
 <type>translate</type>
 <object-name>FDbars</object-name>
 <property>/instrumentation/flightdirector/bar-roll</property>
 <factor>0.0016</factor>
 <axis>
  <x>0</x>
  <y>1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
    <name>Alpha Sort</name>
    <object-name>APspeed</object-name>
    <object-name>APlnav</object-name>
    <object-name>APvnav</object-name>
    <object-name>ASIribbon</object-name>
    <object-name>ALTribbon</object-name>
    <object-name>RAtape</object-name>
    <object-name>VSIscale</object-name>
    <object-name>HDGface</object-name>
    <object-name>AI</object-name>
    <object-name>PFDscreen</object-name>
</animation>

<animation>
   <type>select</type>
   <object-name>risingrunway</object-name>
   <condition>
       <less-than>
         <property>/position/altitude-agl-ft</property>
         <value>200.1</value>
       </less-than>
   </condition>
 </animation>

<animation>
 <type>translate</type>
 <object-name>risingrunway</object-name>
 <property>/position/altitude-agl-ft</property>
 <interpolation>
  <entry>
   <ind> 16.0</ind>
   <dep> -0.032</dep>
  </entry>
  <entry>
   <ind> 200.0</ind>
   <dep> -0.02</dep>
  </entry>
 </interpolation>
 <axis>
  <x>0</x>
  <y>0</y>
  <z>-1</z>
 </axis>
</animation>

<animation>
   <type>select</type>
   <object-name>fd.annun</object-name>
   <condition>
       <equals>
         <property>/instrumentation/flightdirector/fd-on</property>
         <value>1</value>
       </equals>
   </condition>
 </animation>

<animation>
   <type>select</type>
   <object-name>ap.annun</object-name>
   <condition>
       <equals>
         <property>/instrumentation/flightdirector/autopilot-on</property>
         <value>1</value>
       </equals>
   </condition>
 </animation>

 <animation>
   <type>select</type>
   <object-name>GSscale</object-name>
   <object-name>GSneedle</object-name>
   <condition>
     <and>
         <property>/instrumentation/nav/has-gs</property>
         <property>/instrumentation/nav/in-range</property>
     </and>
   </condition>
 </animation>

 <animation>
   <type>translate</type>
   <object-name>GSneedle</object-name>
   <property>/instrumentation/nav/gs-needle-deflection</property>
<interpolation>
     <entry>
       <ind> -10.0</ind>
       <dep>-0.0308</dep>
     </entry>
     <entry>
       <ind> 0.0</ind>
       <dep>0.0</dep>
     </entry>
     <entry>
       <ind> 10.0</ind>
       <dep>0.0308</dep>
     </entry>
</interpolation>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>


 <animation>
   <type>select</type>
   <object-name>Omarker</object-name>
   <condition>
     <equals>
       <property>/instrumentation/marker-beacon/outer</property>
       <value>true</value>
     </equals>
   </condition>
 </animation>
 
 <animation>
   <type>select</type>
   <object-name>Mmarker</object-name>
   <condition>
     <equals>
       <property>/instrumentation/marker-beacon/middle</property>
       <value>true</value>
     </equals>
   </condition>
 </animation>
 
 <animation>
   <type>select</type>
   <object-name>Imarker</object-name>
   <condition>
     <equals>
       <property>/instrumentation/marker-beacon/inner</property>
       <value>true</value>
     </equals>
   </condition>
 </animation>
 
 <animation>
   <type>select</type>
   <object-name>LOCscale</object-name>
   <object-name>LOCneedle</object-name>
   <condition>
     <and>
         <property>/instrumentation/nav/nav-loc</property>
         <property>/instrumentation/nav/in-range</property>
     </and>
   </condition>
 </animation>

 <animation>
   <type>translate</type>
   <object-name>LOCneedle</object-name>
   <property>/instrumentation/nav/heading-needle-deflection</property>
<interpolation>
     <entry>
       <ind> -10.0</ind>
       <dep>-0.0253</dep>
     </entry>
     <entry>
       <ind> 0.0</ind>
       <dep>0.0</dep>
     </entry>
     <entry>
       <ind> 10.0</ind>
       <dep>0.0253</dep>
     </entry>
</interpolation> 
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>



 <animation>
   <type>rotate</type>
   <object-name>pfdhdg</object-name>
   <property>autopilot/internal/fdm-heading-bug-error-deg</property>
   <interpolation>
     <entry>
       <ind> -180.0</ind>
       <dep>-50.0</dep>
     </entry>
     <entry>
       <ind>-50.0</ind>
       <dep>-50.0</dep>
     </entry>
     <entry>
       <ind> 0.0</ind>
       <dep>0.0</dep>
     </entry>
     <entry>
       <ind> 50.0</ind>
       <dep>50.0</dep>
     </entry>
     <entry>
       <ind>180.0</ind>
       <dep>50.0</dep>
     </entry>
   </interpolation>
<axis>
     <x>-1.0</x>
     <y>0.0</y>
     <z>0.0</z>
   </axis>
   <center>
     <x-m>0.007</x-m>
     <y-m>-0.013</y-m>
     <z-m>-0.135</z-m>
   </center>
 </animation>

 <animation>
   <type>texmultiple</type>
   <object-name>AI</object-name>
   <transform>
     <property>/orientation/pitch-deg</property>
     <subtype>textranslate</subtype>
     <factor>0.00314</factor>
     <axis>
       <x>0</x>
       <y>1</y>
       <z>0</z>
     </axis>
   </transform>
   <transform>
     <property>/orientation/roll-deg</property>
     <subtype>texrotate</subtype>
     <center>
       <x>0.25</x>
       <y>0.50</y>
     </center>
     <axis>
       <x>0</x>
       <y>0</y>
       <z>-1</z>
     </axis>
   </transform>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>rollptr</object-name>
   <property>/orientation/roll-deg</property>
   <axis>
     <x>1.0</x>
     <y>0.0</y>
     <z>0.0</z>
   </axis>
   <center>
     <x-m>0.0</x-m>
     <y-m>-0.014</y-m>
     <z-m>0.005</z-m>
   </center>
 </animation>



  <animation>
   <type>texrotate</type>
   <object-name>HDGface</object-name>
   <property>/orientation/heading-magnetic-deg</property>
   <center>
     <x>0.75</x>
     <y>0.75</y>
     <z>0.0</z>
   </center>
   <axis>
     <x>0</x>
     <y>0</y>
     <z>-1</z>
   </axis>
 </animation>
